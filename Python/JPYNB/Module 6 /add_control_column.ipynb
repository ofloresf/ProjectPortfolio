      "source": [
        "\n",
        "import pandas as pd\n",
        "df = pd.read_csv(\"bp.csv\")\n",
        "\n",
        "df.loc[(df['systolic'] < 140) & (df['diastolic'] < 90), 'control'] = 'Y'\n",
        "df.loc[(df['systolic'] >= 140) | (df['diastolic'] >= 90), 'control'] = 'N'\n",
        "\n",
        "df.head()\n",
        "print(df)\n",
        "df.to_csv('bp_control.csv')"
      ]
    }
  ]
}
