      "source": [
        "import pandas as pd\n",
        "\n",
        "df = pd.read_csv(\"bp.txt\", sep=\" \")\n",
        "\n",
        "# split the bp column into two columns at the '/'\n",
        "\n",
        "df[['systolic', 'diastolic']] = df['bp'].str.split('/', expand=True)\n",
        "\n",
        "# view the updated DataFrame\n",
        "\n",
        "print(df)\n",
        "\n",
        "# save the data to a new (CSV) file\n",
        "df.to_csv('bp.csv')"
      ]
    }
  ]
